<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="DOCUMESSAGE" schema="KINGWORD" class="DocuMessage">
		<primary-key>
			<key-column name="DOCUID" property="docuId" />
		</primary-key>
		<column name="DOCUFINISHDATE" property="docuFinishDdate"></column>
		<column name="DOCUISFINISHED" property="docuIsFinished"></column>
		<column name="REMARK" property="remark"></column>
		<column name="SUBMITDATE" property="submitDate"></column>
		<column name="SUBMITOR" property="submitor"></column>
		<column name="SUBMITORPHONE" property="submitorPhone"></column>
		<column name="TYPE" property="type"></column>
		<column name="WORDNAME" property="wordName"></column>
		<column name="WRITER" property="writer"></column>
		<column name="WRITERPHONE" property="writerPhone"></column>
	</table>
	<table name="KINGWORD" schema="KINGWORD" class="KingWord">
		<primary-key>
			<key-column name="KINGWORDID" property="kingWordId" />
		</primary-key>
		<column name="REMARK" property="remark"></column>
		<column name="WORDCREATEDATE" property="wordCreateDate"></column>
		<column name="WORDCREATOR" property="wordCreator"></column>
		<column name="WORDDESC" property="wordDesc"></column>
		<column name="WORDNAME" property="wordName"></column>
		<column name="WORDSTATE" property="wordState"></column>
		<column name="WORDTYPE" property="wordType"></column>
	</table>
	<table name="RELATION_DOCU_WORD" schema="KINGWORD"
		class="RelationDocuWord">
		<primary-key>
			<key-column name="KINGWORDID" property="kingWordId" />
			<key-column name="DOCUID" property="docuId" />
		</primary-key>
		<column name="CREATEDATE" property="createDate"></column>
		<column name="STATE" property="state"></column>
	</table>
	<table name="RELATION_SUB_DOCU" schema="KINGWORD"
		class="RelationSubDocu">
		<primary-key>
			<key-column name="DOCUID" property="docuId" />
			<key-column name="SUBJECTTYPEID" property="subjectTypeId" />
		</primary-key>
	</table>
	<table name="SUBJECTTYPE" schema="KINGWORD" class="SubjectType">
		<primary-key>
			<key-column name="SUBJECTTYPEID" property="subjectTypeId" />
		</primary-key>
		<column name="CREATEDATE" property="createDate"></column>
		<column name="DESCRIPTION" property="description"></column>
		<column name="PARENTTYPEID" property="parentTypeId"></column>
		<column name="REMARK" property="remark"></column>
		<column name="WORDNAME" property="wordName"></column>
	</table>
</hibernate-reverse-engineering>